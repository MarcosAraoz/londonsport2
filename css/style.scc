#zapaspuma, #zapasadidas, #zapasnike {
  display: flex;
  flex-direction: row;
  justify-content: center;
  font-family: Cambria, Cochin, Georgia, Times, "Times New Roman", serif;
}

* {
  margin: 0;
  padding: 0;
}

header {
  background-color: black;
}

.navboton-blanco {
  background-color: white;
}

.jordanlogo {
  width: 80px;
  height: 100px;
}

#letrasblancas {
  color: white;
  font-family: Cambria, Cochin, Georgia, Times, "Times New Roman", serif;
  font-size: larger;
}
#letrasblancas:hover {
  color: darkblue;
}

/*ACA TERMINA EL HEADER*/
/*BODY INDEX*/
.imgpresentacion img {
  width: 100%;
  height: 8%;
}

.mainindex {
  border: solid black 5px;
}
.mainindex h1 {
  font-family: fantasy;
  text-align: center;
  border-style: inset;
}
.mainindex img {
  width: 100px;
  height: 100px;
  margin: 50px 8%;
}

.uno {
  transition: all 1s;
}
.uno:hover {
  transform: scale(2);
}

.dos {
  transition: all 1s;
}
.dos:hover {
  transform: scale(2);
}

.tres {
  transition: all 1s;
}
.tres:hover {
  transform: scale(2);
}

.cuatro {
  transition: all 1s;
}
.cuatro:hover {
  transform: scale(1.5);
}

#sobrenosotros {
  text-align: center;
}

.fondogris {
  background-color: rgba(173, 169, 169, 0.753);
}
.fondogris p {
  font-size: 15px;
  font-family: cursive;
}

form {
  background-image: url(../Imagenes/fondonikezapas.jpg);
  display: flex;
  flex-direction: column;
  margin-left: 20%;
  margin-right: 20%;
  border-style: solid;
  margin-bottom: 15px;
}
form h1 {
  font-family: "Dancing Script", cursive;
  text-align: center;
  background-color: black;
  color: white;
}

.boton_formulario {
  border-radius: 30px;
  background-color: darkblue;
  margin: 10px;
  font-size: large;
  color: white;
}

.boton_formulario:hover {
  background-color: black;
}

/*BODY CALZADO*/
