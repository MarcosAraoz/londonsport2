  flex-wrap: wrap;
  border: solid black 10px;
  background-color: #FFFFFF;
  background-image: linear-gradient(84deg, #FFFFFF 0%, #6284FF 50%, #FF0000 100%);
}
.nikeadidaspuma li {
  list-style: none;
