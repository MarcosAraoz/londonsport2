}

/*CELU INDEX*/
@media screen and (max-width: 1000px) {
  header nav ul {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
  }
  main div {
    display: grid;
    grid-template-rows: 1fr 1fr;
    grid-template-columns: 50% 50% 50%;
    grid-template-areas: "titulo titulo" "uno     dos" "tres cuatro";
  }
  .mainindex h1 {
    grid-area: titulo;
    border-bottom: 5px solid black;
  }
  .uno {
    grid-area: uno;
  }
  .dos {
    grid-area: dos;
  }
  .tres {
    grid-area: tres;
  }
  .cuatro {
    grid-area: cuatro;
  }
  .mainindex img {
    width: 120px;
    height: 120px;
  }
  footer {
    display: flex;
    flex-direction: column;
  }
  .nikeadidaspuma {
    flex-direction: column;
    justify-content: center;
    align-items: center;
    border: solid 5px black;
  }
}

